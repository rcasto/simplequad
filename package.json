{
  "name": "simplequad",
  "version": "1.0.0",
  "description": "A simple QuadTree implementation",
  "main": "dist/simplequad.umd.js",
  "module": "dist/simplequad.esm.js",
  "types": "dist/simplequad.d.ts",
  "scripts": {
    "build": "npm-run-all clean --parallel build:*",
    "build:bundles": "rollup --config",
    "build:bundles-min": "rollup --config --minify",
    "build:types": "dts-generator --prefix simplequad --project . --out dist/simplequad.d.ts",
    "test": "ava",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules",
    "prepublish": "npm-run-all test build"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "repository": {
		"type": "git",
		"url": "https://github.com/rcasto/simplequad.git"
	},
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-typescript": "7.3.3",
    "ava": "2.2.0",
    "dts-generator": "3.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rollup": "1.16.7",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-uglify": "6.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "ava": {
    "files": [
      "test/**/*",
      "!test/util.ts"
    ],
    "sources": [
      "src/**/*"
    ],
    "helpers": [
      "test/util.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
